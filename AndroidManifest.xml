<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.MusikMonksSolution.guitarquiz"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
         <activity
            android:name="de.GuitarQuiz.Activities.StartMenu"
           >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
          android:name="de.GuitarQuiz.Activities.Quiz">
        </activity>
        
        <activity android:name="de.GuitarQuiz.Activities.MenuSelect"></activity>
        <activity android:name="de.GuitarQuiz.Activities.GameOver"></activity>
        <activity android:name="de.GuitarQuiz.Activities.Settings"></activity>
    </application>

</manifest>
